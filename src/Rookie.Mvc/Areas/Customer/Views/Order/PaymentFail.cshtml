@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container m-t-100">
    @if (TempData["ErrorMessage"] != null)
    {
        <div class="alert alert-danger">
            @TempData["ErrorMessage"]
        </div>
    }
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
</div>




